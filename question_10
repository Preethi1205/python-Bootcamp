ans=0
ans1=0

for i in range(len(li)):
    if li[i]>ans:
        ans=li[i]
print(ans)
for i in range(len(li)):
    if ans1<li[i]:
        ans1=li[i]
print(ans1)
sum=(ans+ans1)//2
for i in range(len(li)):
    sum=li[i]
print(li)'''



'''n=int(input())
