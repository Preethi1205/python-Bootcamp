''' find the duplicate numbers in an array'''

#li=list(map(int,input().split()))



li=list(map(int,input().split()))
max=li[0]
ans=0
for i in range(len(li)):
    if max<li[i]:
        max=li[i]
    for j in range(len(li)):
        if(li[j]<max) and (max>ans):
            ans=li[j]
print(ans)



